  "description": "Enhanced Telegram Anonymous Chat Bot with Custom UI",
  "main": "bots.js",
  "scripts": {
    "start": "node bots.js",
    "dev": "node bots.js",
    "admin": "node admin-server.js",
    "all": "node start-all.js",
    "cluster": "pm2 start ecosystem.config.js --env production",
    "cluster:dev": "pm2 start ecosystem.config.js --env development",
